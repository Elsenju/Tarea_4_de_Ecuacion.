#include "conio.h"
#include "stdio.h"
#include "math.h"
main()
{
       int a,b,c;
      float x1,x2,x3;
      printf("teclea los valores de a,b,c\n");
      scanf("%i %i %i",&a,&b,&c);
      x3=sqrt(b*b)-4*a*c;
      x1=(-b-x3)- 4*a*c/(2*a);
      x2=(-b+x3)- 4*a*c/(2*a);
      printf("el valor de x1 es %.2f ",x1);
      printf("\nel valor de x2 es %.2f ",x2);
      if(x3<0)
      printf("\nla ecuacion es imaginaria");
      else
      printf("\nla ecuacion es real");
      if(x3==0)
       printf("\nla ecuacion es compleja");
      getch();
      }
